<h1>RESULTS PAGE</h1>
<ul>
    <% data["Search"].forEach(function(movie) { %>
        <li>
            <img src="<%= movie["Poster"] %>" width="30" height="44">
            <strong>
                - <%= movie["Title"] %>
            </strong> 
            - <%= movie["Year"] %>
        </li>
    <% }) %>
</ul>

<a href="/">Search Again</a>